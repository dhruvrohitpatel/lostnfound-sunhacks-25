{
  "watch": [
    "*.py",
    "*.db",
    "requirements.txt"
  ],
  "ext": "py,db,txt",
  "ignore": [
    "node_modules",
    "__pycache__",
    "*.pyc",
    ".git",
    "venv",
    "uploads"
  ],
  "exec": "./venv/bin/uvicorn main:app --reload --host 0.0.0.0 --port 8000",
  "env": {
    "NODE_ENV": "development"
  },
  "delay": 1000,
  "killTimeout": 5000,
  "restartable": "rs"
}
